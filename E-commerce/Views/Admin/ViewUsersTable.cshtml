@model List<User>
<table class="table table-striped mt-2 mb-2" style="padding-left:20px;width:max-content">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col" class="text-center">Image</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
            <th scope="col">Address</th>
            <th scope="col">Gender</th>
            <th scope="col">Age</th>
            <th scope="col">AdminOrCustomer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td scope="row" style="padding-left:20px">@user.Id</td>
                <td><img src="~/Images/@user.ImagePath" width="200px" height="200px" /></td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Phone</td>
                <td>@user.Address</td>
                <td>@user.Gender</td>
                <td>@user.Age</td>
                <td style="padding-left:40px">@user.AdminOrCustomer<td>
            </tr>
        }
    </tbody>
</table>
